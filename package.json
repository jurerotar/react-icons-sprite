{
  "name": "react-icons-sprite",
  "version": "0.7.0",
  "type": "module",
  "license": "MIT",
  "description": "",
  "author": "Jure Rotar <hello@jurerotar.com>",
  "homepage": "https://github.com/jurerotar/react-icons-sprite#README",
  "main": "dist/icon.mjs",
  "types": "dist/icon.d.mts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./vite": {
      "types": "./dist/vite/plugin.d.mts",
      "import": "./dist/vite/plugin.mjs",
      "default": "./dist/vite/plugin.mjs"
    },
    "./webpack": {
      "types": "./dist/webpack/plugin.d.mts",
      "import": "./dist/webpack/plugin.mjs",
      "default": "./dist/webpack/plugin.mjs"
    },
    "./webpack/loader": {
      "types": "./dist/webpack/loader.d.mts",
      "import": "./dist/webpack/loader.mjs",
      "default": "./dist/webpack/loader.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurerotar/react-icons-sprite.git"
  },
  "bugs": {
    "url": "https://github.com/jurerotar/react-icons-sprite/issues"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "format": "biome format --no-errors-on-unmatched",
    "format:fix": "biome format --write --no-errors-on-unmatched",
    "lint": "biome lint  --no-errors-on-unmatched",
    "lint:fix": "biome lint --write  --no-errors-on-unmatched",
    "type-check": "tsgo",
    "test": "vitest run",
    "prepublishOnly": "npm run build",
    "release": "npm publish --access public"
  },
  "peerDependencies": {
    "react": ">= 16",
    "react-dom": ">= 16",
    "react-icons": ">= 5"
  },
  "devDependencies": {
    "@babel/generator": "7.29.1",
    "@babel/parser": "7.29.0",
    "@babel/traverse": "7.29.0",
    "@babel/types": "7.29.0",
    "@biomejs/biome": "2.3.14",
    "@types/babel__generator": "7.27.0",
    "@types/babel__traverse": "7.28.0",
    "@types/node": "25.2.2",
    "@types/react-dom": "19.2.3",
    "@typescript/native-preview": "7.0.0-dev.20260210.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-icons": "5.5.0",
    "tsdown": "0.20.3",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vitest": "4.0.18",
    "webpack": "5.105.0"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "webpack",
    "webpack-plugin",
    "rollup",
    "rolldown",
    "react-icons",
    "react",
    "icons",
    "svg",
    "svg-sprite",
    "spritesheet",
    "icon-sprite"
  ]
}
